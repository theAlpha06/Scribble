{
    "name": "scribble",
    "description": "Scribble is a chrome extension to take notes. It is built using React and Webpack.",
    "version": "1.0.1",
    "manifest_version": 3,
    "author": "Shubham Singh",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "action": {
        "default_title": "Make Notes"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}